<%- include("util/_year") %><%
config = config;
mdList = mdList;
mdListStr = JSON.stringify(mdList);
configStr = JSON.stringify(config.param);
copyRightYear = funcCopyRightYear(config);
%><!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="format-detection" content="telephone=no,address=no,email=no">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= config.commons.sitename %> | <%= config.commons.description %></title>
    <meta name="description" content="<%= config.commons.description %>">

    <!-- theme-color -->
    <meta name="theme-color" content="#ccb380">

    <!-- css -->
    <link href="css/index.css" rel="stylesheet">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/earlyaccess/sawarabimincho.css" rel="stylesheet">
</head>
<body data-offset="80" id="index">
<div id="wrapper">

    <!-- header -->
    <header class="header" id="navbar">
        <nav class="navbar">
            <h1 class="navbar_head"><a href="#wrapper"><%= config.commons.sitename %></a></h1>
            <ul class="navList">
<%
var i = 1;
mdList.forEach(function (value, key) {
%>
                <li class="navList_item">
                    <a class="navList_itemlink" href="#pdf_<%= i %>-header"><%= value %></a>
                </li>
<%
    i++;
});
%>
            </ul>
        </nav>
    </header>
    <!-- /header -->

    <!-- main -->
    <main class="main">
        <div class="cushion" aria-hiden="true"></div>
        <h2 class="main_header">PDF一覧</h2>
<%
var i = 1;
mdList.forEach(function (value, key) {
%>
        <section class="container">
            <h3 id="pdf_<%= i %>-header" class="pdf_header"><span class="pdf_headerHead">PDF<%= i %></span><%= value %></h3>
            <div id="pdf_<%= i %>" class="pdfobject_container"></div>
        </section>
<%
    i++;
});
%>
    </main>
    <!-- /main -->

    <div class="returnPageTop"><span>↑</span></div>
    <!-- footer -->
    <footer class="footer">
        <small class="copyRight">Copyright © <%= copyRightYear %> <%= config.commons.author %> All Right Reserved.</small>
    </footer>
    <!-- /footer -->

</div>
<div class="data-field">
    <div id="data-mdlist" class="data-mdlist" data-mdlist="<%= mdListStr %>"></div>
    <div id="data-config" class="data-config" data-config="<%= configStr %>"></div>
</div>
<script src="js/lib.min.js" defer></script>
<script src="js/app.min.js" defer></script>
</body>
</html>