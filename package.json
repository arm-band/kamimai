{
    "name": "kamimai",
    "version": "0.14.3",
    "description": "markdown-pdfを自動実行し、pdfのプレビューを表示するボイラープレート",
    "main": "gulpfile.js",
    "scripts": {
        "100stories": "mkdirp src/md/scss/assets src/md/fonts src/md/fonts/YakuHanJP src/md/fonts/YakuHanMP src/md/fonts/YakuHanRP src/md/scss/assets/css src/md/css dist dist/pdf results",
        "hikuma": "cpx \"./node_modules/yakuhanjp/dist/css/*\" \"./src/md/scss/assets/css\"",
        "aramata": "rename-files yakuhanjp.css _yakuhanjp.scss --PATH=./src/md/scss/assets/css/",
        "kyougoku": "rename-files yakuhanmp.css _yakuhanmp.scss --PATH=./src/md/scss/assets/css/",
        "mizuki": "rename-files yakuhanrp.css _yakuhanrp.scss --PATH=./src/md/scss/assets/css/",
        "kanei2": "cpx \"./node_modules/yakuhanjp/dist/fonts/YakuHanJP/*\" \"./src/md/fonts/YakuHanJP\"",
        "ad1749": "cpx \"./node_modules/yakuhanjp/dist/fonts/YakuHanMP/*\" \"./src/md/fonts/YakuHanMP\"",
        "endofmay": "cpx \"./node_modules/yakuhanjp/dist/fonts/YakuHanRP/*\" \"./src/md/fonts/YakuHanRP\"",
        "sanmoto": "npm-run-all -s 100stories hikuma endofmay kanei2 ad1749 aramata kyougoku mizuki",
        "gorouzaemon": "npm run gulp",
        "start": "npm-run-all -s sanmoto gorouzaemon",
        "restart": "gulp",
        "mallet": "gulp mallet",
        "gulp": "gulp",
        "budayu": "rimraf ./src/js/concat/ ./src/md/scss/assets/ ./src/md/css/ ./src/md/fonts/ ./dist/ ./results/ ./package-lock.json"
    },
    "author": "Arm Band",
    "license": "ISC",
    "devDependencies": {
        "normalize.preprocessor": "^8.0.0",
        "browser-sync": "^2.26.13",
        "cpx": "^1.5.0",
        "mkdirp": "^1.0.4",
        "npm-run-all": "^4.1.5",
        "rename-files-cli": "^1.0.1",
        "rimraf": "^3.0.2",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-ejs": "^5.1.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^4.1.0",
        "sass": "^1.32.0",
        "fibers": "^5.0.0",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-uglify-es": "^2.0.0",
        "jquery": "^3.5.1",
        "jquery.easing": "^1.4.1",
        "markdown-pdf": "^10.0.0",
        "pdfobject": "^2.2.4",
        "yakuhanjp": "^3.3.1",
        "yaml": "1.10.0"
    }
}
